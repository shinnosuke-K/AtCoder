name=""
num=""

abc:
	mkdir ./ABC/${num}
	touch ./ABC/${num}/A.py
	touch ./ABC/${num}/B.py
	touch ./ABC/${num}/C.py
	touch ./ABC/${num}/D.py
	touch ./ABC/${num}/E.py
	touch ./ABC/${num}/F.py

agc:
	mkdir ./AGC/${num}
	touch ./AGC/${num}/A.py
	touch ./AGC/${num}/B.py
	touch ./AGC/${num}/C.py
	touch ./AGC/${num}/D.py
	touch ./AGC/${num}/E.py
	touch ./AGC/${num}/F.py

other:
	mkdir ./${name}/${num}
	touch ./${name}/${num}/A.py
	touch ./${name}/${num}/B.py
	touch ./${name}/${num}/C.py
	touch ./${name}/${num}/D.py
	touch ./${name}/${num}/E.py
	touch ./${name}/${num}/F.py